{
  "name": "@synonymdev/pubky",
  "type": "module",
  "description": "Pubky client",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pubky/pubky"
  },
  "scripts": {
    "lint": "standard --fix",
    "test": "brittle test/*.js -cov",
    "preinstall": "cargo run --bin bundle_pubky_npm",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "files": [
    "nodejs/*",
    "index.js",
    "browser.js"
  ],
  "main": "index.js",
  "browser": "browser.js",
  "types": "pubky.d.ts",
  "sideEffects": [
    "./snippets/*"
  ],
  "keywords": [
    "web",
    "dht",
    "dns",
    "decentralized",
    "identity"
  ],
  "devDependencies": {
    "brittle": "^3.6.1",
    "standard": "^17.1.0"
  }
}
